#include<stdio.h>

int main()

{
    int A, B, C;
    char S , Q;
    scanf("%d %c %d %c %d",&A, &S, &B, &Q, &C);

    if ( S == '+' && Q == '=')
    {
        if ( C == (A+B))
        {
           printf("Yes\n");
        }
        else
        {
            printf("%d\n",A+B);
        }
    }
    if ( S == '-' && Q == '=')
    {
        if ( C == (A-B))
        {
           printf("Yes\n");
        }
        else
        {
            printf("%d\n",A-B);
        }
    }
    if ( S == '*' && Q == '=')
    {
        if ( C == (A*B))
        {
           printf("Yes\n");
        }
        else
        {
            printf("%d\n",A*B);
        }
    }

}
