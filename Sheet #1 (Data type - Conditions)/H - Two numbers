#include <stdio.h>
#include <math.h>

int main()
{
    float n, m, div;
    scanf("%f %f", &n, &m);
    div = n / m;
    printf("floor %.0f / %.0f = %.0f\n", n, m, floor(div));
    printf("ceil %.0f / %.0f = %.0f\n", n, m, ceil(div));
    printf("round %.0f / %.0f = %.0f\n", n, m, round(div));
    return 0;
}

